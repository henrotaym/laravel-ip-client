{
    "name": "henrotaym/laravel-ip-client",
    "description": "Helpers concerning client performing requests.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^2.0",
        "henrotaym/laravel-api-client": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Henrotaym\\LaravelTrustupIoIpClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Henrotaym\\LaravelTrustupIoIpClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Henrotaym\\LaravelTrustupIoIpClient\\Providers\\LaravelTrustupIoIpClientServiceProvider"
            ]
        }
    }
